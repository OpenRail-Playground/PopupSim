{
  "name": "pop-up-sim-cc",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10.0.0"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "coverage": "npm run test:unit -- --coverage",
    "preview": "vite preview --port 4173",
    "test": "npm run test:unit",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:debug": "npm run test:e2e -- --debug",
    "test:e2e:trace": "npm run test:e2e -- --trace on",
    "test:e2e:report": "playwright show-report",
    "test:a11y": "npx pa11y-ci",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:styles": "npx stylelint \"src/**/*.{vue,css,scss}\"",
    "format": "prettier --write src/",
    "lhci": "npm run lhci:collect && npm run lhci:assert && npm run lhci:open",
    "lhci:open": "lhci open",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:autorun": "lhci assert"
  },
  "dependencies": {
    "@db-ui/core": "3.0.0",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "pinia": "2.1.7",
    "vue": "3.4.23",
    "vue-i18n": "10.0.0",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.8.5",
    "@intlify/unplugin-vue-i18n": "5.0.0",
    "@lhci/cli": "0.13.0",
    "@pinia/testing": "0.1.3",
    "@playwright/test": "1.47.1",
    "@rushstack/eslint-patch": "1.7.2",
    "@tsconfig/node20": "20.1.2",
    "@types/jsdom": "21.1.6",
    "@types/node": "20.11.25",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "2.1.1",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "0.5.1",
    "axe-core": "4.8.4",
    "eslint": "8.57.0",
    "eslint-plugin-playwright": "1.5.2",
    "eslint-plugin-vue": "9.22.0",
    "jsdom": "25.0.0",
    "npm-run-all2": "6.1.2",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "rollup-plugin-license": "3.2.0",
    "sass": "1.71.1",
    "start-server-and-test": "2.0.3",
    "stylelint": "16.2.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.0.0",
    "typescript": "5.4.2",
    "vite": "5.1.5",
    "vite-plugin-vue-devtools": "7.3.7",
    "vitest": "2.1.1",
    "vue-tsc": "2.0.6"
  }
}
