{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "i18n-ally.localesPaths": "src/locales",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.preferredDelimiter": ".",
  "i18n-ally.pathMatcher": "{locale}.yml",
  "i18n-ally.dirStructure": "file",
  "i18n-ally.displayLanguage": "de",
  "i18n-ally.editor.preferEditor": true,
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.extract.keyMaxLength": 40,
  "i18n-ally.sourceLanguage": "de",
  "i18n-ally.sortKeys": true,
  "files.associations": {
    ".env.local.template": "dotenv"
  },
  "stylelint.validate": ["css", "scss", "vue"]
}
